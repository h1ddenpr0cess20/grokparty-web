{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src"],
  "tsconfig": "tsconfig.app.json",
  "out": "docs/api",
  "exclude": [
    "**/*.test.ts",
    "**/*.test.tsx",
    "src/vite-env.d.ts"
  ],
  "excludeExternals": false,
  "excludePrivate": true,
  "excludeProtected": false,
  "hideGenerator": true,
  "categorizeByGroup": true,
  "includeVersion": false
}
